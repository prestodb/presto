{
  "catalogs": [
    {
      "allow": true
    }
  ],
  "principals": [
    {
      "principal": "(.*)",
      "principal_to_user": "$1",
      "allow": true
    },
    {
      "principal": "([^/]+)/?.*@.*",
      "principal_to_user": "$1",
      "allow": true
    },
    {
      "principal": "invalid/?.*@.*",
      "user": ".*",
      "allow": false
    },
    {
      "principal": "special/?(.*)@.*",
      "principal_to_user": "$1",
      "allow": true
    },
    {
      "principal": "valid/?.*@.*",
      "user": ".*",
      "allow": true
    }
  ]
}
