{
  "queues": {
    "user-${USER}": {
      "maxConcurrent": 3,
      "maxQueued": 3
    },
    "adhoc-${USER}": {
      "maxConcurrent": 3,
      "maxQueued": 3
    },
    "dashboard-${USER}": {
      "maxConcurrent": 1,
      "maxQueued": 1
    },
    "global": {
      "maxConcurrent": 100,
      "maxQueued": 1000
    }
  },
  "rules": [
    {
      "source": "(?i).*dashboard.*",
      "queues": [
        "dashboard-${USER}",
        "user-${USER}",
        "global"
      ]
    },
    {
      "queues": [
        "adhoc-${USER}",
        "user-${USER}",
        "global"
      ]
    }
  ]
}
